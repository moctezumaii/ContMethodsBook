@article{davidsenMigratoryBehaviourSurvival2009a,
  title = {Migratory Behaviour and Survival Rates of Wild Northern {{Atlantic}} Salmon {{Salmo}} Salar Post-Smolts: Effects of Environmental Factors},
  shorttitle = {Migratory Behaviour and Survival Rates of Wild Northern {{Atlantic}} Salmon {{Salmo}} Salar Post-Smolts},
  author = {Davidsen, J. G. and Rikardsen, A. H. and Halttunen, E. and Thorstad, E. B. and {\O}Kland, F. and Letcher, B. H. and Skar{\dh}hamar, J. and N{\ae}sje, T. F.},
  year = 2009,
  journal = {Journal of Fish Biology},
  volume = {75},
  number = {7},
  pages = {1700--1718},
  issn = {1095-8649},
  doi = {10.1111/j.1095-8649.2009.02423.x},
  urldate = {2024-06-17},
  abstract = {To study smolt behaviour and survival of a northern Atlantic salmon Salmo salar population during river descent, sea entry and fjord migration, 120 wild S. salar were tagged with acoustic tags and registered at four automatic listening station arrays in the mouth of the north Norwegian River Alta and throughout the Alta Fjord. An estimated 75\% of the post-smolts survived from the river mouth, through the estuary and the first 17 km of the fjord. Survival rates in the fjord varied with fork length (LF), and ranged from 97{$\cdot$}0 to 99{$\cdot$}5\% km-1. On average, the post-smolts spent 1{$\cdot$}5 days (36 h, range 11--365 h) travelling from the river mouth to the last fjord array, 31 km from the river mouth. The migratory speed was slower (1{$\cdot$}8 LF s-1) in the first 4 km after sea entry compared with the next 27 km (3{$\cdot$}0 LF s-1). Post-smolts entered the fjord more often during the high or ebbing tide (70\%). There was no clear diurnal migration pattern within the river and fjord, but most of the post-smolts entered the fjord at night (66\%, 2000--0800 hours), despite the 24 h daylight at this latitude. The tidal cycle, wind-induced currents and the smolts' own movements seemed to influence migratory speeds and routes in different parts of the fjord. A large variation in migration patterns, both in the river and fjord, might indicate that individuals in stochastic estuarine and marine environments are exposed to highly variable selection regimes, resulting in different responses to environmental factors on both temporal and spatial scales. Post-smolts in the northern Alta Fjord had similar early marine survival rates to those observed previously in southern fjords; however, fjord residency in the north was shorter.},
  copyright = {\copyright{} 2009 The Authors. Journal compilation \copyright{} 2009 The Fisheries Society of the British Isles},
  langid = {english},
  keywords = {acoustic telemetry,diurnal migration,horizontal distribution,migratory speed,Program MARK,sea entry},
  file = {C\:\\Users\\amolina6\\Zotero\\storage\\26D22FBS\\Davidsen et al. - 2009 - Migratory behaviour and survival rates of wild nor.pdf;C\:\\Users\\amolina6\\Zotero\\storage\\UBBFGSEA\\j.1095-8649.2009.02423.html}
}

@article{davisHabitatUseTuxedo2010,
