@article{fissetteSensoryTrapLeads2024,
  title = {Sensory Trap Leads to Reliable Communication without a Shift in Nonsexual Responses to the Model Cue},
  author = {Fissette, Skye D. and Buchinger, Tyler J. and Tamrakar, Sonam and Scott, Anne M. and Li, Weiming},
  year = 2024,
  month = mar,
  journal = {BEHAVIORAL ECOLOGY},
  volume = {35},
  number = {2},
  pages = {arae006},
  publisher = {Oxford Univ Press Inc},
  address = {Cary},
  issn = {1045-2249, 1465-7279},
  doi = {10.1093/beheco/arae006},
  urldate = {2024-12-17},
  abstract = {The sensory trap model of signal evolution suggests that males manipulate females into mating using traits that mimic cues used in a nonsexual context. Despite much empirical support for sensory traps, little is known about how females evolve in response to these deceptive signals. Female sea lamprey (Petromyzon marinus) evolved to discriminate a male sex pheromone from the larval odor it mimics and orient only toward males during mate search. Larvae and males release the attractant 3-keto petromyzonol sulfate (3kPZS), but spawning females avoid larval odor using the pheromone antagonist, petromyzonol sulfate (PZS), which larvae but not males, release at higher rates than 3kPZS. We tested the hypothesis that migratory females also discriminate between larval odor and the male pheromone and orient only to larval odor during anadromous migration, when they navigate within spawning streams using larval odor before they begin mate search. In-stream behavioral assays revealed that, unlike spawning females, migratory females do not discriminate between mixtures of 3kPZS and PZS applied at ratios typical of larval versus male odorants. Our results indicate females discriminate between the sexual and nonsexual sources of 3kPZS during but not outside of mating and show sensory traps can lead to reliable sexual communication without females shifting their responses in the original context. Males in some species signal to females using traits that mimic nonsexual cues, but how females evolve in response to being manipulated remains poorly understood. Using in-stream behavioral assays, we show that female sea lamprey use a mimetic male pheromone for reliable sexual communication, without shifting their response to the deceptive component in the nonsexual migratory context.},
  langid = {english},
  keywords = {BENEFITS,BIASES,chemical ecology,COSTS,ECOLOGY,FEMALE SEA LAMPREY,PHEROMONE,PHYLOGENETIC DISTRIBUTION,receiver bias,sensory trap,signal evolution},
  annotation = {Web of Science ID: WOS:001166351700002},
  file = {C:\Users\amolina6\Zotero\storage\84VVXTLT\Fissette et al. - 2024 - Sensory trap leads to reliable communication without a shift in nonsexual responses to the model cue.pdf}
}

