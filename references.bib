@article{rungeeLongTermAssessmentClimate2017,
  title = {Long-{{Term Assessment}} of {{Climate Change Impacts}} on {{Tennessee Valley Authority Reservoir Operations}}: {{Norris Dam}}},
  shorttitle = {Long-{{Term Assessment}} of {{Climate Change Impacts}} on {{Tennessee Valley Authority Reservoir Operations}}},
  author = {Rungee, Joseph and Kim, Ungtae},
  year = 2017,
