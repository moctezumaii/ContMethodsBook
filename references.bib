@article{postNutrientExcretionRates2009,
  title = {Nutrient {{Excretion Rates}} of {{Anadromous Alewives}} during {{Their Spawning Migration}}},
  author = {Post, David M. and Walters, Annika W.},
  year = 2009,
  month = mar,
  journal = {Transactions of the American Fisheries Society},
  volume = {138},
