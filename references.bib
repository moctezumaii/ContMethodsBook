@article{dobieszChangesMidsummerWater2009,
  title = {Changes in Mid-Summer Water Temperature and Clarity across the {{Great Lakes}} between 1968 and 2002},
  author = {Dobiesz, Norine E. and Lester, Nigel P.},
  year = 2009,
  month = sep,
  journal = {Journal of Great Lakes Research},
  volume = {35},
  number = {3},
  pages = {371--384},
  issn = {0380-1330},
  doi = {10.1016/j.jglr.2009.05.002},
  urldate = {2025-07-31},
  abstract = {In recent decades, three important events have likely played a role in changing the water temperature and clarity of the Laurentian Great Lakes: 1) warmer climate, 2) reduced phosphorus loading, and 3) invasion by European Dreissenid mussels. This paper compiled environmental data from government agencies monitoring the middle and lower portions of the Great Lakes basin (lakes Huron, Erie and Ontario) to document changes in aquatic environments between 1968 and 2002. Over this 34-year period, mean annual air temperature increased at an average rate of 0.037~{$^\circ$}C/y, resulting in a 1.3~{$^\circ$}C increase. Surface water temperature during August has been rising at annual rates of 0.084~{$^\circ$}C (Lake Huron) and 0.048~{$^\circ$}C (Lake Ontario) resulting in increases of 2.9~{$^\circ$}C and 1.6~{$^\circ$}C, respectively. In Lake Erie, the trend was also positive, but it was smaller and not significant. Water clarity, measured here by August Secchi depth, increased in all lakes. Secchi depth increased 1.7~m in Lake Huron, 3.1~m in Lake Ontario and 2.4~m in Lake Erie. Prior to the invasion of Dreissenid mussels, increases in Secchi depth were significant (p{$<$}0.05) in lakes Erie and Ontario, suggesting that phosphorus abatement aided water clarity. After Dreissenid mussel invasion, significant increases in Secchi depth were detected in lakes Ontario and Huron.},
  keywords = {Climate change,Dreissenid mussels,Laurentian Great Lakes,Secchi depth,Water clarity,Water temperature},
  file = {C\:\\Users\\amolina6\\Zotero\\storage\\Y6SE8IWW\\Dobiesz and Lester - 2009 - Changes in mid-summer water temperature and clarity across the Great Lakes between 1968 and 2002.pdf;C\:\\Users\\amolina6\\Zotero\\storage\\39PTLXRR\\S0380133009001294.html}
}

