@article{sorensenMixtureNewSulfated2005,
  title = {Mixture of New Sulfated Steroids Functions as a Migratory Pheromone in the Sea Lamprey},
  author = {Sorensen, P. W. and Fine, J. M. and Dvornikovs, V. and Jeffrey, C. S. and Shao, F. and Wang, J. Z. and Vrieze, L. A. and Anderson, K. R. and Hoye, T. R.},
