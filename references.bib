  langid = {english},
  keywords = {community,Etheostoma,flow,riffle,substrate},
  file = {C\:\\Users\\amolina6\\Zotero\\storage\\MUG49FS6\\Thompson and Stallsmith - 2016 - Microhabitat Partitioning of an Assemblage of Dart.pdf;C\:\\Users\\amolina6\\Zotero\\storage\\V7DDM6VM\\rra.html}
